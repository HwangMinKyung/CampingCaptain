<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- resultType 등록 -->
	<typeAliases>
		<typeAlias type="com.camping.biz.dto.NoticeVO" alias="notice" />
		<typeAlias type="com.camping.biz.dto.QuestionsVO" alias="question" />
		<typeAlias type="com.camping.biz.dto.UsersVO" alias="users" />
		<typeAlias type="com.camping.biz.dto.CampOrderVO" alias="campOrder" />
		<typeAlias type="com.camping.biz.dto.CampingVO" alias="camping" />
		<typeAlias type="com.camping.biz.dto.QnaVO" alias="qna" />
		<typeAlias type="com.camping.biz.dto.RealReviewVO" alias="reviews"/>
		<typeAlias type="com.camping.biz.dto.AdminVO" alias="admin"/>
		<typeAlias type="com.camping.biz.dto.AdminNoticeVO" alias="adminnotice"/>
		<typeAlias type="com.camping.biz.dto.CampOrderCancelVO" alias="cancel"/>
		<typeAlias type="com.camping.biz.dto.TempOrderVO" alias="tempOrder" />
		<typeAlias type="com.camping.biz.dto.PayVO" alias="pay" />
	</typeAliases>

	<!-- SQL 매핑파일 등록 -->
	<mappers>
		<mapper resource="mappings/notice-mapping.xml" />
		<mapper resource="mappings/adminnotice-mapping.xml" />
		<mapper resource="mappings/question-mapping.xml" />
		<mapper resource="mappings/users-mapping.xml" />
		<mapper resource="mappings/campOrder-mapping.xml" />
		<mapper resource="mappings/camping-mapping.xml" />
		<mapper resource="mappings/qna-mapping.xml" />
		<mapper resource="mappings/realreview-mapping.xml" />
		<mapper resource="mappings/admin-mapping.xml" />
		<mapper resource="mappings/campOrderCancel-mapping.xml" />
		<mapper resource="mappings/tempOrder-mapping.xml" />
		<mapper resource="mappings/pay-mapping.xml" />
		<mapper resource="mappings/calculate-mapping.xml" />
	</mappers>
	
</configuration>